#!/bin/bash

# Check if the CFILE variable is set
if [ -z "$CFILE" ]; then
  echo "Error: CFILE variable is not set."
  exit 1
fi

# Extract the base name of the C file (without extension)
BASENAME=$(basename "$CFILE" .c)

# Generate the assembly code in Intel syntax
gcc -S -masm=intel "$CFILE" -o "${BASENAME}.s"

echo "Assembly code generated in ${BASENAME}.s"